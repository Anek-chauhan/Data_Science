    "def find_price(price,discount=10):\n",
    "    dis=(price*discount)/100\n",
    "    final_price=price-dis\n",
    "    return final_price\n",
    "price=int(input(\"Enter the price\"))\n",
    "discount=int(input(\"Enter the Discount\"))\n",
    "find_price(price,discount)"
